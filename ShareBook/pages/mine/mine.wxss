.book_information{
  display: block;
  margin-top: 15.1rpx;
}
.name_box{
  padding-left: 32rpx;
  padding-top: 17rpx;
  padding-bottom: 17rpx;
  display: flex;
}
.book_information_name{
  color: #999999;
  font-size: 26rpx;
  font-family: "PingFangSC-Regular";
}
.list-item{
  margin-left: 0rpx;
  margin-bottom: 1rpx;
  background-color:#FFFFFF;
  overflow: hidden;
  display: flex;
  height: auto;
  position: relative;
}
.navigator{
  position: relative;
  display: flex;
  height: 90rpx;
  width: 100vw;
  align-items:center;
}
.navigator-icon{
  width: 40rpx;
  height: 46rpx;
  margin-top: auto;
  margin-bottom: auto;
  margin-left: 42rpx;
}
.navigator-name{
  margin-left: 40rpx;
  margin-top: auto;
  margin-bottom: auto;
  flex: 1;
}
.navigator-name text{
  font-size: 34rpx;
  color: #333333;
}
.navigator-info{
  margin-right: 68rpx;
}
.navigator-info text{
  font-size: 32rpx;
  color: #999999;
}
.navigator-arrow {
  /* padding-left: 32rpx; */
  position: relative;
}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color: #888888;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 32rpx;
}
.function_name{
  color: #999999;
  font-size: 26rpx;
  font-family: "PingFangSC-Regular";
}